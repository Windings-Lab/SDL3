{
  "version": 6,
  "configurePresets": [
    {
      "name": "user-default",
      "hidden": true,
      "environment": {
        "GNU_PATH": "D:/ImpPrograms/msys64/mingw64/bin"
      },
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "D:/ImpPrograms/vcpkg/scripts/buildsystems/vcpkg.cmake",
        "VCPKG_TARGET_TRIPLET": "x64-windows-static-md"
      }
